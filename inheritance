
class Animal:
    def __init__(self, name):
        self.name = name

    def sound(self):
        return "This animal makes a sound."

class Cat(Animal):
    def sound(self):
        return f"{self.name} says: Meow!"

class Dog(Animal):
    def sound(self):
        return f"{self.name} says: Woof!"

class Cow(Animal):
    def sound(self):
        return f"{self.name} says: Moo!"

animals = [
    Cat("Kitty"),
    Dog("Buddy"),
    Cow("Daisy"),
]

for animal in animals:
    print(animal.sound())
